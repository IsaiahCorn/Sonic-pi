use_bpm 160
use_synth :tech_saws
live_loop :things do
  with_fx :wobble do
    play :c4
    sleep 0.5
    play :e4
    sleep 0.5
    play :g4
    sleep 0.5
    play :b4
    sleep 0.5
    play :c4
    sleep 0.5
    play :b4
    sleep 0.5
    play :g4
    sleep 0.5
    play:e4
    sleep 0.5
  end
end



